{
  "presets": [
    ["@babel/env", {
      "targets": {
        "edge": "17",
        "firefox": "61",
        "chrome": "67",
        "safari": "11.1",
        "ie": "11"
      },
      "useBuiltIns": "entry"
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-export-default-from",
    "@babel/plugin-proposal-export-namespace-from"
  ]
}
